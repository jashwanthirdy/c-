#include<iostream>
using namespace std;
int main()
{
    int num1,num2;
    cout<<"Enter the two-floating numbers: ";
    cin>>num1>>num2;
    cout<<"Addition: "<<num1+num2<<endl;
    cout<<"Subraction: "<<num1-num2<<endl;
    cout<<"Multiplication: "<<num1*num2<<endl;
    if(num2!=0)
    {
        cout<<"Division: "<<num1/num2<<endl;
        cout<<"Modulo: "<<fmod(num1,num2)<<endl;
    }
    else
    {
        cout<<"Division and modulo are undefined by zero"<<endl;
    }
    return 0;
}
