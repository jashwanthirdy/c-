#include<iostream>
using namespace std;
int main()
{
    int n, sum=0, digits;
    cout<<"Enter a number: ";
    cin>>n;
    int original_n = n;
    while(n>0)
    {
        digits = n % 10;
        sum += digits;
        n = n / 10;
    }
    cout<<"Sum of digits of"<<original_n<<" is: "<<sum<<endl;
    return 0;
}
